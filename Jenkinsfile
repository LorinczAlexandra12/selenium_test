pipeline{
  
  agent any
  
  stages{
  
    stage("Init and check out repository"){
      steps{
        echo 'Initializing and checking out repository...'
      }
    }
    stage("Build project"){
      steps{
        echo 'Building the project...'
      }
    }
    stage("Run tests"){
      steps{
        echo 'Running the tests...'
      }
    }
    stage("Clean out workspace"){
      steps{
       echo 'Cleaning out the workspace...'
      }
    }
  }  
}
